{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "lib": ["ES2019", "DOM"],
    "types": ["node"],
